<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pkg.GameController">
    <children>
      <VBox id="VBox" alignment="CENTER" prefHeight="200.0" prefWidth="320.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <ScrollPane prefHeight="150.0" prefWidth="320.0">
              <content>
                <AnchorPane prefHeight="178.0" prefWidth="297.0">
                     <children>
                        <TextArea id="result" fx:id="result" disable="true" editable="false" focusTraversable="false" prefHeight="178.0" prefWidth="303.0">
                           <cursor>
                              <Cursor fx:constant="OPEN_HAND" />
                           </cursor>
                        </TextArea>
                     </children>
                  </AnchorPane>
              </content>
            </ScrollPane>
            <TextField id="choice" fx:id="choice" onAction="#nextButton" prefHeight="25.0" prefWidth="196.0" />
            <Button id="go" mnemonicParsing="false" onAction="#nextButton" text="Go" />
         </children>
      </VBox>
    </children>
</AnchorPane>
